# Set figure size to 5x4 inches with white backgroundfigure.figsize:                 6,4             # figure size in inchesfigure.facecolor:               ffffff           # figure facecolorfigure.dpi:                     300figure.constrained_layout.use:  True# Change to cyan, magenta, yellow and black color paletteaxes.prop_cycle:   cycler('color', ['6da7de', 'ee266d', 'dee000', '5ea15d','d82222','943fa6','63c5b5','ff38ba','eb861e','9e0059'])# Remove top and right spinesaxes.spines.left:   False            # display axis spinesaxes.spines.bottom: Trueaxes.spines.top:    Falseaxes.spines.right:  Falseaxes.edgecolor:     666666axes.linewidth:     1axes.axisbelow:     True# gridaxes.grid.axis:     yaxes.grid:          Truegrid.color:         d8d8d8# Both axis tick customisationsxtick.direction:    out              # direction: {in, out, inout}ytick.direction:    in              # direction: {in, out, inout}xtick.color:        666666xtick.bottom:       Truextick.major.width:  2               # major tick width in pointsxtick.major.size:   4ytick.major.width:  2               # major tick width in pointsxtick.labelsize:    medium          # fontsize of the tick labelsytick.labelsize:    medium          # fontsize of the tick labels# Change font and font size of title and both axis labelsfont.family:        Tahomafont.size:          14.0axes.titlesize:     x-large         # fontsize of the axes titleaxes.labelsize:     large           # fontsize of the x and y labels